<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<BorderPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="garden_planner.gui.Controller">
   <center>
      <Pane fx:id="garden" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
   </center>
   <left>
      <HBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="200.0" prefWidth="150.0" BorderPane.alignment="CENTER">
         <children>
            <VBox minHeight="-Infinity" minWidth="-Infinity" prefHeight="200.0" prefWidth="60.0">
               <children>
                  <Label text="Top">
                     <font>
                        <Font name="Arial" size="13.0" />
                     </font></Label>
                  <Label text="Height">
                     <font>
                        <Font name="Arial" size="13.0" />
                     </font></Label>
                  <Button fx:id="submit_final" mnemonicParsing="false" text="Submit">
                     <font>
                        <Font name="Arial" size="13.0" />
                     </font></Button>
               </children>
            </VBox>
            <VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="200.0" prefWidth="100.0">
               <children>
                  <TextField fx:id="bedTopValue">
                     <font>
                        <Font name="Arial" size="13.0" />
                     </font></TextField>
                  <TextField>
                     <font>
                        <Font name="Arial" size="13.0" />
                     </font></TextField>
               </children>
            </VBox>
         </children>
      </HBox>
   </left>
</BorderPane>
